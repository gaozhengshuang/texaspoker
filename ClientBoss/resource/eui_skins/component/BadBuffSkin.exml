<?xml version="1.0" encoding="utf-8"?>
<e:Skin class="BadBuffSkin" width="200" height="200" xmlns:e="http://ns.egret.com/eui" xmlns:w="http://ns.egret.com/wing" xmlns:tween="egret.tween.*" >
	<w:Declarations>
		<tween:TweenGroup id="boxshake">
			<tween:TweenItem target="{boxBuffImg}">
				<tween:Wait duration="150"/>
				<tween:Set>
					<tween:props>
						<e:Object rotation="{349}"/>
					</tween:props>
				</tween:Set>
				<tween:Wait duration="150"/>
				<tween:Set>
					<tween:props>
						<e:Object rotation="{0}"/>
					</tween:props>
				</tween:Set>
				<tween:Wait duration="150"/>
				<tween:Set>
					<tween:props>
						<e:Object rotation="{11}"/>
					</tween:props>
				</tween:Set>
				<tween:Wait duration="150"/>
				<tween:Set>
					<tween:props>
						<e:Object rotation="{360}"/>
					</tween:props>
				</tween:Set>
			</tween:TweenItem>
		</tween:TweenGroup>
	</w:Declarations>
	<e:Image source="ui_json.boxBuffBg" scaleX="1" scaleY="1" horizontalCenter="0" verticalCenter="0" touchEnabled="false"/>
	<e:Image id="boxBuffImg" source="ui_json.boxBuff1" scaleX="1" scaleY="1" touchEnabled="true" anchorOffsetX="100" height="200" anchorOffsetY="100" horizontalCenter="3" verticalCenter="-5"/>
	<e:ProgressBar id="badBuffProgressBar" width="94" height="25" horizontalCenter="0" x="-246" scaleX="1" scaleY="1" verticalCenter="65" touchEnabled="false" skinName="BadBuffProgressBar"/>
	<e:Label text="拆弹奖励" textColor="0xB14DDC" italic="true" size="18" scaleX="1" scaleY="1" horizontalCenter="0" verticalCenter="85.5" touchEnabled="false"/>




</e:Skin>