		<e:Image y="15" horizontalCenter="0" source="safe_bxx_png" />
		<e:Image y="120" source="pub_biankuang1_png" width="598" horizontalCenter="0" scale9Grid="25,23,15,7" />
		<ns1:TabComponent id="saveTab" y="120.5" skinName="TabComponentSkin" horizontalCenter="0" />
