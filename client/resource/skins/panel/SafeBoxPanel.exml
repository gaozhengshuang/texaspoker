			<ns1:GreenShadowButton id="withdrawAddBtn" label="+" x="540" y="217"  skinName="commonButtonGY60x60Skin"/>
			<ns1:ShadowButton id="withdrawBtn" label="取出" y="460" anchorOffsetX="0" rotation="359.86" horizontalCenter="0"
