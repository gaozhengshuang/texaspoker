        xmlns:w="http://ns.egret.com/wing" xmlns:ns1="*" xmlns:tween="egret.tween.*">
	<w:Config id="15c813d330f" />
	<e:Group id="tweenGroup" width="666" height="900" anchorOffsetY="0" x="0" y="0">
