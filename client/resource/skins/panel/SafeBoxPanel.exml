			<e:Label text="当前金币：" x="0" y="0" scaleX="1" scaleY="1" size="26" />
			<e:Label text="保险箱金币：" x="0" y="50" scaleX="1" scaleY="1" size="26" />
			<e:Label id="currentGold" text="99999999" x="130" y="0" scaleX="1" scaleY="1" size="26" />
			<e:Label id="saveGold" text="999900" x="150" y="50" scaleX="1" scaleY="1" size="26" />
		</e:Group>
		<e:Group id="saveGoldGroup" width="666" height="620" anchorOffsetX="0" anchorOffsetY="0" horizontalCenter="0"
