			<e:Label text="新密码" y="90" size="25" width="150" height="50" textAlign="center" verticalAlign="middle" x="30" />
			<e:Label text="再输入一次" size="25" y="160" width="150" height="50" textAlign="center" verticalAlign="middle" x="30" />
			<e:Label id="tipPwd" text="Label" y="230" size="28" horizontalCenter="0" textColor="0xFF0000" />
			<e:Group id="pwdGroup" anchorOffsetY="0" x="220" y="20" scaleX="0.7" scaleY="0.7">
				<e:layout>
					<e:VerticalLayout gap="35" />
				</e:layout>
			</e:Group>
			<ns1:ShadowButton id="modifyPwdBtn" label="修改密码" anchorOffsetX="0" horizontalCenter="0" y="280"
			                  skinName="commonButtonY2Skin" />
		</e:Group>
		<ns1:ShadowButton id="closeButton" label="Button" x="600" y="65" scaleY="1" anchorOffsetY="27" anchorOffsetX="27"
		                  scaleX="1" skinName="closeButtonSkin" />
