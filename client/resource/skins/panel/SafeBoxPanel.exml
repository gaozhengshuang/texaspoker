			         y="470" size="25" width="600" horizontalCenter="0" lineSpacing="10" textColor="0xcccccc" />
			<e:Label id="buyVIPlabel" text="续费VIP " x="359.17" y="504.96" size="25" border="false" textColor="0xfad050" />
		</e:Group>
		<e:Group id="withdrawGroup" width="666" height="620" anchorOffsetX="0" anchorOffsetY="0" name="提现" y="200"
