			<ns1:GreenShadowButton id="subBtn" label="-" x="65" y="217" rotation="359.86"  width="60" height="60" skinName="commonButtonGY60x60Skin"/>
			<ns1:GreenShadowButton id="addBtn" label="+" x="540" y="217"  width="60" height="60" skinName="commonButtonGY60x60Skin"/>
			<ns1:ShadowButton id="saveBtn" label="存入" y="320" anchorOffsetX="0" rotation="359.86" horizontalCenter="0"
